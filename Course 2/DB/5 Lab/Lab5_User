--Задание 2
CREATE TABLE TRA_T1 (
    ID NUMBER PRIMARY KEY,
    NAME VARCHAR(20)
);

INSERT INTO TRA_T1 VALUES (15, 'Andrey');
INSERT INTO TRA_T1 VALUES (11, 'Dmitry');
INSERT INTO TRA_T1 VALUES (12, 'Nick');
COMMIT;

SELECT * FROM TRA_T1;

--Задание 6
DROP TABLE TRA_T1;

--Задание 7
SELECT * FROM USER_RECYCLEBIN;

--Задание 8
FLASHBACK TABLE TRA_T1 TO BEFORE DROP;

--Задание 9
begin
FOR loopIndex IN 0..99999
LOOP
   INSERT INTO TRA_T1 VALUES (loopIndex + 20, 'NAME');
END LOOP;
COMMIT;
end;

--Задание 10
SELECT COUNT(*) FROM TRA_T1;

--Задание 12
SELECT ID, ROWID, ORA_ROWSCN FROM TRA_T1
    ORDER BY ID
    FETCH FIRST 10 ROWS ONLY;

DROP TABLE TRA_T1;

CREATE TABLE TRA_T1 (
    ID NUMBER PRIMARY KEY,
    NAME VARCHAR(20)
) ROWDEPENDENCIES;