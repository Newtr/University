-- Создание объектной таблицы для "Сотрудники"
CREATE TABLE Сотрудники_obj OF t_Сотрудник;

-- Создание новой таблицы
CREATE TABLE Сотрудники_obj_new (ИДЕНТИФИКАТОР_СОТРУДНИКА NUMBER, ФИО_СОТРУДНИКА VARCHAR2(100 BYTE), КОНТРАКТ NUMBER, РОЛЬ VARCHAR2(100 BYTE), ДАТА_РОЖДЕНИЯ DATE, ПОЛ CHAR(255 BYTE), ОТДЕЛ VARCHAR2(100 BYTE),
Сотрудник_данные t_Сотрудник
);

-- Копирование данных из реляционной таблицы в объектную таблицу
INSERT INTO Сотрудники_obj
SELECT t_Сотрудник(ИДЕНТИФИКАТОР_СОТРУДНИКА, ФИО_СОТРУДНИКА, КОНТРАКТ, РОЛЬ, ДАТА_РОЖДЕНИЯ, ПОЛ, ОТДЕЛ)
FROM Сотрудники;

-- Копирование данных из старой таблицы в новую
INSERT INTO Сотрудники_obj_new
SELECT ИДЕНТИФИКАТОР_СОТРУДНИКА, ФИО_СОТРУДНИКА, КОНТРАКТ, РОЛЬ, ДАТА_РОЖДЕНИЯ, ПОЛ, ОТДЕЛ, t_Сотрудник(ИДЕНТИФИКАТОР_СОТРУДНИКА, ФИО_СОТРУДНИКА, КОНТРАКТ, РОЛЬ, ДАТА_РОЖДЕНИЯ, ПОЛ, ОТДЕЛ)
FROM Сотрудники_obj;


-- Если все в порядке, вы можете удалить старую таблицу
DROP TABLE Сотрудники_obj;

-- И переименовать новую таблицу в Сотрудники_obj
ALTER TABLE Сотрудники_obj_new RENAME TO Сотрудники_obj;

