-- Создание индекса для атрибута
CREATE INDEX idx_ИМЯ_КАНДИДАТА ON Собеседования_obj (ИМЯ_КАНДИДАТА);

-- Создание индекса для метода
CREATE INDEX Индекс_по_РЕЗУЛЬТАТУ_Собеседования ON Собеседования_obj(Собеседование_данные.get_result());

SELECT index_name 
FROM all_indexes 
WHERE table_name = 'СОБЕСЕДОВАНИЯ_OBJ';
