-- Создание объектной таблицы для "Собеседования"
CREATE TABLE Собеседования_obj OF t_Собеседование;

-- Создание новой таблицы
CREATE TABLE Собеседования_obj_new (ИДЕНТИФИКАТОР_СОБЕСЕДОВАНИЯ NUMBER, ИДЕНТИФИКАТОР_ВАКАНСИИ NUMBER, ИМЯ_КАНДИДАТА VARCHAR2(255 BYTE), ДАТА_СОБЕСЕДОВАНИЯ DATE, РЕЗУЛЬТАТ_СОБЕСЕДОВАНИЯ VARCHAR2(255 BYTE), СОТРУДНИК_ПРИНИМАВШИЙ_СОБЕСЕДОВАНИЕ VARCHAR2(255 BYTE),
Собеседование_данные t_Собеседование
);

-- Копирование данных из реляционной таблицы в объектную таблицу
INSERT INTO Собеседования_obj
SELECT t_Собеседование(ИДЕНТИФИКАТОР_СОБЕСЕДОВАНИЯ, ИДЕНТИФИКАТОР_ВАКАНСИИ, ИМЯ_КАНДИДАТА, ДАТА_СОБЕСЕДОВАНИЯ, РЕЗУЛЬТАТ_СОБЕСЕДОВАНИЯ, СОТРУДНИК_ПРИНИМАВШИЙ_СОБЕСЕДОВАНИЕ)
FROM Собеседования;

-- Копирование данных из старой таблицы в новую
INSERT INTO Собеседования_obj_new
SELECT ИДЕНТИФИКАТОР_СОБЕСЕДОВАНИЯ, ИДЕНТИФИКАТОР_ВАКАНСИИ, ИМЯ_КАНДИДАТА, ДАТА_СОБЕСЕДОВАНИЯ, РЕЗУЛЬТАТ_СОБЕСЕДОВАНИЯ, СОТРУДНИК_ПРИНИМАВШИЙ_СОБЕСЕДОВАНИЕ, t_Собеседование(ИДЕНТИФИКАТОР_СОБЕСЕДОВАНИЯ, ИДЕНТИФИКАТОР_ВАКАНСИИ, ИМЯ_КАНДИДАТА, ДАТА_СОБЕСЕДОВАНИЯ, РЕЗУЛЬТАТ_СОБЕСЕДОВАНИЯ, СОТРУДНИК_ПРИНИМАВШИЙ_СОБЕСЕДОВАНИЕ)
FROM Собеседования_obj;

-- Если все в порядке, вы можете удалить старую таблицу
DROP TABLE Собеседования_obj;

-- И переименовать новую таблицу в Собеседования_obj
ALTER TABLE Собеседования_obj_new RENAME TO Собеседования_obj;
