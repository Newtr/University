DELETE FROM Сотрудники
WHERE (Идентификатор_сотрудника, Контракт) IN (
  SELECT Идентификатор_сотрудника, Контракт
  FROM (
    SELECT Идентификатор_сотрудника, Контракт,
           ROW_NUMBER() OVER (PARTITION BY Контракт ORDER BY Контракт) AS НомерСтроки
    FROM Сотрудники
  )
  WHERE НомерСтроки > 1
);
