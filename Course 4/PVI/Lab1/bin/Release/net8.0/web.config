<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<handlers>
			<!-- Настройка для HTTP-обработчика -->
			<add name="HandlerHTTP" path="*.TRA" verb="*" type="Lab1.HandlerHTTP, Lab1" resourceType="Unspecified" />
		</handlers>

		<!-- Настройки для перенаправления запросов к Kestrel (ASP.NET Core) -->
		<aspNetCore processPath="dotnet" arguments=".\Lab1.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="InProcess" />
	</system.webServer>
</configuration>
